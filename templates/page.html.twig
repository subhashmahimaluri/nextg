<header id="navbar" role="banner" class="navbar container navbar-default">  
  <div class="container">
    <!-- .btn-navbar is used as the toggle for collapsed navbar content -->
    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
    <i class="fa fa-bars"></i>
    </button>
    <div class="navbar-header">
      {% if page.header %}
        {{ page.header }}
      {% endif %}
    </div>
     <div class="search-icon">
      <a href="#searchbox" class="">
        <i class="fa fa-search"></i>
      </a>
    </div>
  <div id="search-wrapper" class="nectar">   
  <div id="search">      
    <div class="container">          
         <div id="search-box">          
          <div class="col span_12">
              {% if page.search %}
                {{ page.search }}
              {% endif %}
          </div><!--/span_12-->           
          <ul class="ui-autocomplete ui-front ui-menu ui-widget ui-widget-content ui-corner-all" id="ui-id-1" tabindex="0" style="display: none; top: 55px;"></ul>
        </div><!--/search-box-->         
         <div id="close"><a href="#"><i class="fa fa-times"></i></span></a></div>      
     </div><!--/container-->      
  </div><!--/search-->    
</div>  
{% if page.primary_menu %}
  <div class="navbar-collapse">
    {{ page.primary_menu }}   
  </div>
{% endif %}
</div>  
</header>
<div class="main-container container">
<div class="row">
  <main id="content" class="column main-content" role="main">
  <section class="section">
    <a id="main-content" tabindex="-1"></a>
    {{ page.content }}
  </section>
  </main>
  {% if page.sidebar_first %}
    <div id="sidebar-first" class="column sidebar">
      <aside class="section" role="complementary">
        {{ page.sidebar_first }}
      </aside>
    </div>
  {% endif %}
  {% if page.sidebar_second %}
    <div id="sidebar-second" class="column sidebar">
      <aside class="section" role="complementary">
        {{ page.sidebar_second }}
      </aside>
    </div>
  {% endif %}
</div>
</div>
<footer class="footer">
{% if page.footer %}
  <div class="footer-top">
    <div class="container">
      {{ page.footer }}
    </div>    
  </div>
{% endif %}
{% if page.footer_bottom %}
  <div class="footer-bottom">
    <div class="container">
      {{ page.footer_bottom }}
    </div>    
  </div>
{% endif %}
</footer>